{
  "extends": ["next/core-web-vitals", "airbnb-base"],
  "rules": {
    "semi": "off",
    "no-unused-vars": "warn",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ]
  }
}
