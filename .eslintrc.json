{
  "extends": ["next/core-web-vitals", "airbnb-base"],
  "rules": {
    "import/no-named-as-default": 0,
    "linebreak-style": 0,
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": { "multiline": true, "minProperties": 1 }
      }
    ],
    "semi": "off",
    "no-unused-vars": "warn",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ]
  }
}
